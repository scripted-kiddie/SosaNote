{
  "manifest_version": 3,
  "name": "SosaNote 2.1 beta",
  "version": "2.1",
  "description": "Store local notes per website domain.",
  "permissions": ["storage", "tabs", "activeTab"],

  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "iconbeta.png",
      "32": "iconbeta.png",
      "48": "iconbeta.png",
      "128": "iconbeta.png"
    }
  },

  "icons": {
    "16": "iconbeta.png",
    "32": "iconbeta.png",
    "48": "iconbeta.png",
    "128": "iconbeta.png"
  }
}
